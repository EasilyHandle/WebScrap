-events */\n  <div onClick={() => handleClick(item.permalink)}>\n    <ItemPrice\n      {...item.price}\n      discountSource={item.discount_source}\n      from={item.from}\n      showOriginalValue={!!item.price.original_price}\n    />\n  </div>\n);\n\nconst renderPriceWithoutLink = (item) => (\n  <div className=\"ui-search-price__part-without-link\">\n    <ItemPrice\n      {...item.price}\n      discountSource={item.discount_source}\n      from={item.from}\n      showOriginalValue={!!item.price.original_price}\n    />\n  </div>\n);\n\n\nconst renderPrice = (item, options) => {\n  const cardSupportsLinks = !!(options && options.cardSupportsLinks);\n  if (!has(item, 'price.amount')) {\n    return <div className=\"ui-search-price__part\">{item.price.label.text}</div>;\n  }\n  return ((cardSupportsLinks && item.tags != null && item.tags.includes('product_item'))\n    ? renderPriceWithLink(item)\n    : renderPriceWithoutLink(item)\n  );\n};\n\nconst renderBrand = (brand) => {\n  const brandContent = <ItemBrandDiscoverability brand={brand} />;\n  return brandContent;\n};\n\nconst renderTitle = (title, layout, permalink, target, isAd) => {\n  const titleContent = <ItemTitle title={title} target={target} />;\n  switch (layout) {\n    case GRID:\n      return titleContent;\n    case STACK:\n    default:\n      return renderLinkTitle(permalink, title, titleContent, target, isAd);\n  }\n};\n\nconst renderHighlightGroup = ({ highlight }) => highlight && (\n  <Group noSeparator className={`${namespace}__highlight-container`}>\n    <HightlightLabel {...highlight} />\n  </Group>\n);\n\nconst renderMediaTagGroup = ({ media }) => (\n  media && (\n    <Group noSeparator name={CARD_DATA_GROUPS.MEDIA_TAG}>\n      <HightlightLabel {...media} />\n    </Group>\n  )\n);\n\nconst renderRentalTypeGroup = ({ rental_type: rentalType }) => {\n  if (!rentalType) {\n    return null;\n  }\n\n  const { secondary_label: hint, ...label } = rentalType;\n\n  return (\n    <HighlightGroup\n      name={C