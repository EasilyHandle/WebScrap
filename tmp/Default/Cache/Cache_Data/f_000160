+json\"\n      // eslint-disable-next-line react/no-danger\n      dangerouslySetInnerHTML={{ __html: serialize(data, { isJSON: true }) }}\n    />\n  </Head>\n);\n\nconst SchemaContainer = props => {\n  const seo = props;\n  const { schema } = seo;\n\n  let formatedData = {};\n  let productBrand;\n\n  if (schema && schema.product) {\n    if (schema.product.brand_attribute) {\n      productBrand = {\n        '@type': 'Brand',\n        name: schema.product.brand_attribute.name,\n      };\n    }\n\n    formatedData = {\n      '@context': 'https://schema.org',\n      '@graph': [\n        {\n          '@type': 'SearchResultsPage',\n          id: schema.search.url,\n          name: seo.h1,\n          about: seo.description,\n        },\n        {\n          '@type': 'ItemList',\n          url: schema.product.url,\n          id: `${schema.search.url}/#itemList`,\n          numberOfItems: 1,\n          itemListElement: [\n            {\n              '@type': 'ListItem',\n              position: 1,\n              item: {\n                '@type': 'Product',\n                name: schema.product.name,\n                url: schema.search.url,\n                aggregateRating: {\n                  '@type': 'AggregateRating',\n                  ratingCount: schema.product.aggregate_rating.rating_count,\n                  bestRating: 5,\n                  ratingValue: schema.product.aggregate_rating.rating_value,\n                  worstRating: 1,\n                },\n                offers: {\n                  '@type': 'AggregateOffer',\n                  price: schema.product.item_offered.price,\n                  lowPrice: schema.product.item_offered.price,\n                  priceCurrency: schema.product.item_offered.price_currency,\n                  offerCount: 1,\n                  availability: 'https://schema.org/InStock',\n                  itemCondition: 'https://schema.org/NewCondition',\n                },\n                brand: productBrand,\n              },\n   