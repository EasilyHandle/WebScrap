types');\nconst classnames = require('classnames');\nconst CarouselPaged = require('ui-library_ml/ui/carousel');\nconst useCollapseCarousel = require('../../hooks/use-collapse-carousel');\n\nconst namespace = 'ui-search-carousel';\n\nconst collapseCarousel = (handleCollapse) => {\n  const collapse = (e) => {\n    const target = e.target.parentElement;\n    const collapsed = target.classList.contains(`${namespace}--collapsed`);\n    const actionType = collapsed ? 'open' : 'close';\n\n    switch (actionType) {\n      case 'close':\n        target.classList.add(`${namespace}--collapsed`);\n        break;\n      default:\n        target.classList.remove(`${namespace}--collapsed`);\n        break;\n    }\n    handleCollapse();\n  };\n\n  const handleEnterPress = (ev) => {\n    /* istanbul ignore next */\n    if (ev.key === 'Enter') {\n      collapse(ev);\n    }\n  };\n\n  return (\n    /* eslint-disable-next-line */\n    <span className={`${namespace}__btn-collapse`} \n      aria-label=\"reduce carrousel\"\n      role=\"button\"\n      tabIndex={0}\n      onKeyPress={handleEnterPress}\n      onClick={collapse}\n    />\n  );\n};\n\nconst Carousel = ({ slides, settings, className, slideChange, collapsible, state }) => {\n  const [collapsed, handleCollapse] = useCollapseCarousel(state);\n  const carouselStatus = collapsed ? `${namespace}--collapsed` : '';\n\n  return (\n    <div className={classnames(namespace, 'swiper-container', className, carouselStatus)}>\n      {collapsible && collapseCarousel(handleCollapse)}\n      <CarouselPaged\n        {...settings}\n        slides={slides}\n        afterChange={(index) => { slideChange(index); }}\n      />\n    </div>\n  );\n};\n\nCarousel.propTypes = {\n  children: node,\n  className: string,\n  collapsible: bool,\n  rebuildOnUpdate: bool,\n  settings: objectOf(any).isRequired,\n  shouldSwiperUpdate: bool,\n  slideChange: func.isRequired,\n  slides: arrayOf().isRequired,\n  state: bool,\n};\n\nCarousel.defaultProps = {\n  childr