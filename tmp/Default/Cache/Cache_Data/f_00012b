ared {\n    @content;\n\n    // Only print mobile if the context is also mobile\n  } @else if $context == mobile and $type == mobile {\n    @content;\n\n    // Only print desktop if the context is also desktop\n  } @else if $context == desktop and $type == desktop {\n    @content;\n\n    // If context is responsive, print mobile as is and desktop with media query\n  } @else if $context == responsive {\n    @if $type == mobile {\n      @content;\n    } @else {\n      @media (min-width: $breakpoint-desktop) {\n        @content;\n      }\n    }\n  }\n}\n","@include bundle($type: 'desktop') {\n  .andes-pagination {\n    display: block;\n    margin: $pagination-gutter-desktop;\n\n    .andes-pagination__button:not(.andes-pagination__button--back):not(.andes-pagination__button--next):not(.andes-pagination__button--current) {\n      display: inline-block;\n    }\n\n    &__button {\n      .andes-pagination__link {\n        @include square($pagination-button-size-desktop);\n        padding: $pagination-gutter-padding-button-desktop;\n      }\n    }\n\n    &__button--next,\n    &__button--back {\n      .andes-pagination__link {\n        width: $pagination-button-large-size-desktop;\n        padding: $pagination-gutter-padding-arrows-button-desktop;\n      }\n    }\n  }\n}\n\n","@include bundle($type: 'shared') {\n  .#{$checkbox-prefix} {\n    position: relative;\n    cursor: pointer;\n    font-family: $font-family-primary;\n    -webkit-font-smoothing: antialiased;\n    font-size: $checkbox-default-font-size;\n\n    &:not(div) {\n      display: inline-flex;\n    }\n\n    &__input {\n      @include square($checkbox-default-size);\n      @include add-focus-visible;\n\n      position: absolute;\n      left: 0;\n      top: 0;\n      margin: 10px 0 0;\n      transform: translateY(-50%);\n      appearance: none;\n      -webkit-appearance: none;\n      -moz-appearance: none;\n      // Fix focus in Android\n      outline: none;\n\n      &:before {\n        @include square($checkbox-def